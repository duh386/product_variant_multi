<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        
        <record id="create_product_form_view" model="ir.ui.view">
            <field name="name">create.product.form</field>
            <field name="model">create.product</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Create product">
                    <group>
	                    <field name="tmpl_id" widget="selection_create_product" />
	                    <field name="multi" />
                    </group>
                    <group>
                        <button name="do_refresh_view" string="Refresh" type="object" default_focus="1" icon="gtk-jump-to" invisible="1" />
                        <newline/>
                        <button special="cancel" string="Отмена" type="object"/>
                        <newline/>
                        <button name="do_create_product" string="Создать продукт" type="object" default_focus="1"/>
                    </group>
                </form>
            </field>
        </record>
        
        
        
    </data>
</openerp>



